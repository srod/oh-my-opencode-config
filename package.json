{
  "name": "oh-my-opencode-config",
  "version": "0.4.0",
  "description": "Interactive CLI for managing model assignments in oh-my-opencode.json",
  "type": "module",
  "imports": {
    "#backup/*": {
      "types": "./src/backup/*",
      "source": "./src/backup/*",
      "default": "./dist/backup/*"
    },
    "#cli/*": {
      "types": "./src/cli/*",
      "source": "./src/cli/*",
      "default": "./dist/cli/*"
    },
    "#config/*": {
      "types": "./src/config/*",
      "source": "./src/config/*",
      "default": "./dist/config/*"
    },
    "#diff/*": {
      "types": "./src/diff/*",
      "source": "./src/diff/*",
      "default": "./dist/diff/*"
    },
    "#errors/*": {
      "types": "./src/errors/*",
      "source": "./src/errors/*",
      "default": "./dist/errors/*"
    },
    "#models/*": {
      "types": "./src/models/*",
      "source": "./src/models/*",
      "default": "./dist/models/*"
    },
    "#profile/*": {
      "types": "./src/profile/*",
      "source": "./src/profile/*",
      "default": "./dist/profile/*"
    },
    "#prompts/*": {
      "types": "./src/prompts/*",
      "source": "./src/prompts/*",
      "default": "./dist/prompts/*"
    },
    "#test-utils/*": {
      "types": "./src/test-utils/*",
      "source": "./src/test-utils/*",
      "default": "./dist/test-utils/*"
    },
    "#types/*": {
      "types": "./src/types/*",
      "source": "./src/types/*",
      "default": "./dist/types/*"
    },
    "#update/*": {
      "types": "./src/update/*",
      "source": "./src/update/*",
      "default": "./dist/update/*"
    },
    "#utils/*": {
      "types": "./src/utils/*",
      "source": "./src/utils/*",
      "default": "./dist/utils/*"
    },
    "#validation/*": {
      "types": "./src/validation/*",
      "source": "./src/validation/*",
      "default": "./dist/validation/*"
    }
  },
  "bin": {
    "oh-my-opencode-config": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/srod/oh-my-opencode-config.git"
  },
  "homepage": "https://github.com/srod/oh-my-opencode-config",
  "keywords": [
    "opencode",
    "config",
    "cli",
    "ai",
    "agent",
    "model",
    "oh-my-opencode"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "bun --hot --conditions=source src/index.ts",
    "test": "bun test --conditions=source",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format src",
    "format:fix": "biome format --write src",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "typecheck": "tsc --noEmit",
    "sync:agents:check": "bun --conditions=source src/scripts/sync-agent-defaults.ts --check",
    "sync:agents:apply": "bun --conditions=source src/scripts/sync-agent-defaults.ts --apply",
    "sync:cli-version": "bun --conditions=source src/scripts/sync-cli-version.ts",
    "changeset": "changeset",
    "version-packages": "changeset version && bun run sync:cli-version",
    "release": "bun run build && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/bun": "latest",
    "@types/node": "^25.1.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "execa": "^9.6.1",
    "zod": "^4.3.6"
  }
}
